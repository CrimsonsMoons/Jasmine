-- Step 1: Get the "Add" Remote (23rd)
local addRemote = game:GetService("ReplicatedStorage")
    :WaitForChild("Communication", 9e9)
    :WaitForChild("Events", 9e9)
    :GetChildren()[23]

-- Step 2: Get the tab folder
local tabFolder = game.Players.LocalPlayer.PlayerGui
    :WaitForChild("Menus")
    :WaitForChild("Backpack")
    :WaitForChild("Main")
    :WaitForChild("Inventory")
    :WaitForChild("Content")
    :WaitForChild("List")

-- Step 3: Loop through UUIDs without "Locked" and fire up to 25 times
local count = 0
local max = 25

for _, tab in ipairs(tabFolder:GetChildren()) do
    local list = tab:FindFirstChild("List")
    if list then
        for _, item in ipairs(list:GetChildren()) do
            if typeof(item.Name) == "string" and item.Name:match("{.+}") then
                if not item:FindFirstChild("Locked") then
                    print("Firing 'Add' on:", item.Name)
                    addRemote:FireServer("Add", item.Name)
                    count += 1
                    if count >= max then
                        print("Reached limit of 25. Stopping.")
                        return
                    end
                end
            end
        end
    end
end
